  `education` varchar(255) DEFAULT NULL,
  `vehicle` varchar(255) DEFAULT NULL,
  `apply_in` varchar(255) DEFAULT NULL,
  `apply_title` varchar(255) DEFAULT NULL,
  `description` text DEFAULT NULL,
  `date_start` date DEFAULT NULL,
  `date_end` date DEFAULT NULL,
  `supplement_start` date DEFAULT NULL,
  `supplement_end` date DEFAULT NULL,
  `date_publish` varchar(255) DEFAULT NULL,
  `attach_zh` varchar(255) DEFAULT NULL,
  `attach_en` varchar(255) DEFAULT NULL,
  `attach_pt` varchar(255) DEFAULT NULL,
  `progress` tinyint(1) DEFAULT NULL,
  `active` tinyint(1) DEFAULT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

