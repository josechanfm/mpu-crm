-- --------------------------------------------------------

--
-- Table structure for table `enquiry_questions`
--

CREATE TABLE `enquiry_questions` (
  `id` int(11) NOT NULL,
  `enquiry_id` bigint(20) UNSIGNED NOT NULL,
  `parent_id` bigint(20) UNSIGNED DEFAULT NULL,
  `enquiry_response_id` bigint(20) UNSIGNED DEFAULT NULL,
  `content` text DEFAULT NULL,
  `is_closed` tinyint(1) DEFAULT 0,
  `closed_at` datetime DEFAULT NULL,
  `day_used` int(11) DEFAULT NULL,
  `admin_id` bigint(45) UNSIGNED DEFAULT NULL,
  `token` varchar(45) DEFAULT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

