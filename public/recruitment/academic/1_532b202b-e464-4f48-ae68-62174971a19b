-- --------------------------------------------------------

--
-- Table structure for table `gpdps`
--

CREATE TABLE `gpdps` (
  `id` bigint(20) UNSIGNED NOT NULL,
  `staff_num` varchar(10) DEFAULT NULL,
  `name_zh` varchar(255) DEFAULT NULL,
  `name_fr` varchar(255) DEFAULT NULL,
  `id_num` varchar(255) DEFAULT NULL,
  `current_department` varchar(255) DEFAULT NULL,
  `current_position` varchar(255) DEFAULT NULL,
  `original_department` varchar(255) DEFAULT NULL,
  `original_position` varchar(255) DEFAULT NULL,
  `employment_type` varchar(255) DEFAULT NULL,
  `date_start` date NOT NULL,
  `date_remind` date NOT NULL,
  `date_due` date NOT NULL,
  `email` varchar(255) NOT NULL,
  `is_valid` tinyint(1) NOT NULL DEFAULT 0,
  `remark` text DEFAULT NULL,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  `updated_at` datetime NOT NULL DEFAULT current_timestamp()
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

--
-- Dumping data for table `gpdps`
--

